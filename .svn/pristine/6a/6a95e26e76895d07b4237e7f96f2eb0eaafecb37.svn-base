package net.chinaott.cms.core.basicinfo.entity;

public class PublishCdnRef {
    
    private Integer relId;

    private Integer publishPlatformId;

    private Integer cdnPlatformId;

    private String cdnPlatformName;

    private Integer isCdnInject;

    private Integer isOperator;

    public Integer getRelId() {
        return relId;
    }

    public void setRelId(Integer relId) {
        this.relId = relId;
    }

    public Integer getPublishPlatformId() {
        return publishPlatformId;
    }

    public void setPublishPlatformId(Integer publishPlatformId) {
        this.publishPlatformId = publishPlatformId;
    }

    public Integer getCdnPlatformId() {
        return cdnPlatformId;
    }

    public void setCdnPlatformId(Integer cdnPlatformId) {
        this.cdnPlatformId = cdnPlatformId;
    }

    public String getCdnPlatformName() {
        return cdnPlatformName;
    }

    public void setCdnPlatformName(String cdnPlatformName) {
        this.cdnPlatformName = cdnPlatformName == null ? null : cdnPlatformName.trim();
    }

    public Integer getIsCdnInject() {
        return isCdnInject;
    }

    public void setIsCdnInject(Integer isCdnInject) {
        this.isCdnInject = isCdnInject;
    }

    public Integer getIsOperator() {
        return isOperator;
    }

    public void setIsOperator(Integer isOperator) {
        this.isOperator = isOperator;
    }
}